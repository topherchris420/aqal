{
  "name": "aqal-integral-app",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise-grade AQAL Integral Development Studio for personal and organizational growth",
  "keywords": [
    "integral",
    "aqal",
    "personal-development",
    "enterprise",
    "next.js",
    "typescript"
  ],
  "homepage": "https://aqal-integral.com",
  "bugs": {
    "url": "https://github.com/your-org/aqal-integral-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/aqal-integral-app.git"
  },
  "license": "MIT",
  "author": "AQAL Integral Development Team",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "db:migrate": "echo 'Run database migrations'",
    "db:reset": "echo 'Reset database (development only)'",
    "db:seed": "echo 'Seed database with sample data'",
    "deploy:prod": "vercel --prod",
    "deploy:staging": "vercel --target staging",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "npm run type-check",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "prepare": "husky install",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
